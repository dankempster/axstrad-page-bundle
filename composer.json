{
    "name": "axstrad/page-bundle",
    "description": "Simple (crude would be another word) Page bundle for Symfony 2",
    "type": "symfony-bundle",
    "keywords": ["axstrad", "webpage"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Kempster",
            "email": "dev@dankempster.co.uk",
            "role": "Developer"
        }
    ],

    "require": {
        "php": ">=5.4",
        "axstrad/common": ">=0.2.x-dev",
        "axstrad/content": "0.4.x-dev",
        "axstrad/doctrine-extensions-bundle": ">=0.2.x-dev",
        "axstrad/doctrine-extensions": "0.2.*",
        "axstrad/extra-framework-bundle": "dev-develop@dev",
        "axstrad/seo-bundle": ">=0.1.x-dev",
        "doctrine/orm": "~2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "stof/doctrine-extensions-bundle": "~1.1",
        "symfony/symfony": "~2.3, <2.7.x-dev"
    },

    "require-dev": {
        "axstrad/test": ">=0.1.x-dev",
        "axstrad/test-bundle": ">=0.1.x-dev",
        "burgov/key-value-form-bundle": "~1.1",
        "doctrine/doctrine-fixtures-bundle": "~2.2.0",
        "phpunit/phpunit": "~4.1",
        "satooshi/php-coveralls": "dev-master",
        "sensio/framework-extra-bundle": "~2.3 || ~3",
        "sonata-project/admin-bundle": "~2.3",
        "sonata-project/core-bundle": "~2.2",
        "sonata-project/doctrine-orm-admin-bundle": "~2.3.0"
    },

    "autoload": {
        "psr-4": {
            "Axstrad\\Bundle\\PageBundle\\": ""
        }
    },

    "minimum-stability": "dev",

    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev",
            "dev-develop": "0.2.x-dev"
        }
    },

    "archive": {
        "exclude": ["!vendor", "Tests", "phpunit.xml*", ".gitignore", ".travis.yml"]
    }
}
