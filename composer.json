{
    "name": "axstrad/browser-sync-bundle",
    "description": "Simple (crude would be another word) Page bundle for Symfony 2",
    "type": "symfony-bundle",
    "keywords": ["axstrad", "webpage"],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Dan Kempster",
            "email": "dev@dankempster.co.uk",
            "role": "Developer"
        }
    ],

    "require": {
        "php": ">=5.4",
        "axstrad/content": "0.1.*",
        "axstrad/doctrine-extensions-bundle": "0.1.*",
        "axstrad/seo-bundle": "0.1.*",
        "doctrine/orm": "~2.3",
        "doctrine/doctrine-bundle": "~1",
        "stof/doctrine-extensions-bundle": "~1.1",
        "symfony/symfony": "~2.3"
    },

    "require-dev": {
        "axstrad/test": "0.1.*",
        "axstrad/test-bundle": "0.1.*",
        "burgov/key-value-form-bundle": "~1.1@dev",
        "doctrine/doctrine-fixtures-bundle": "2.2.*",
        "phpunit/phpunit": "~4.1",
        "sensio/framework-extra-bundle": "~2.3",
        "sonata-project/admin-bundle": "~2.3",
        "sonata-project/core-bundle": "~2.2",
        "sonata-project/doctrine-orm-admin-bundle": "2.3.*"
    },

    "autoload": {
        "psr-4": {
            "Axstrad\\Bundle\\PageBundle\\": ""
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true,

    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev",
            "dev-develop": "0.2.x-dev"
        }
    },

    "archive": {
        "exclude": ["!vendor", "Tests", "phpunit.xml*", ".gitignore", ".travis.yml"]
    }
}
